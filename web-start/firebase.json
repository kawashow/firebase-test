{
  "hosting": {
    "public": "./public",
    "headers": [
      {
        "source": "**/*.@(js|html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0"
          }
        ]
      }
    ]
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
